{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    },
    {
      "src": "/books",
      "dest": "/index.js"
    },
    {
      "src": "/books/(.*)",
      "dest": "/index.js"
    }
  ]
}
